<form script="components/datepicker/datepicker_main.js" theme="light">
  <label>Custom Datepicker</label>
  <init>
    <set token="show">true</set>
    <!--<eval token="form.customSingleDatepicker">now()</eval>
    <eval token="form.customRangeDatepicker.earliest">relative_time(now(), "-3mon@d")</eval>
    <eval token="form.customRangeDatepicker.latest">relative_time(now(), "-1d@d")</eval>-->
  </init>
  <description>custom datepicker</description>
  <fieldset submitButton="true" autoRun="true"></fieldset>
  <row>
    <panel id="customFieldset">
      <title>Timerange Slider</title>
      <input id="original" type="time" token="originalTime">
        <label>Original Timepicker</label>
        <default>
            <earliest>-24h@h</earliest>
            <latest>now</latest>
        </default>
      </input>
      <html>
        <!-- Anker fÃ¼r JS-Component-->
        <div id="customSingleDatepicker"></div>


        <div id="customRangeDatepicker"></div>
      </html>
    </panel>
  </row>
</form>